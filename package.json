{
  "name": "monday-react-quickstart-app",
  "version": "1.2.4",
  "private": true,
  "scripts": {
    "start": "npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "watch": "npm run stop && concurrently \"npm run server\" \"npm run expose:app\"",
    "deploy:build": "vite build",
    "deploy:push": "mapps code:push --client-side -d \"build\"",
    "deploy": "npm run deploy:build && npm run deploy:push",
    "init": "mapps init -l",
    "expose": "mapps tunnel:create -p 8301 -a 10909418",
    "expose:app": "mapps tunnel:create -p 8301 -a 10909418",
    "server": "vite",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stop": "kill-port 8301 && kill-port 4049 && kill-port 4040",
    "build": "vite build"
  },
  "dependencies": {
    "@mondaycom/apps-sdk": "^2.1.2",
    "@vibe/core": "^3.19.0",
    "@vitejs/plugin-react": "^4.3.2",
    "monday-sdk-js": "^0.5.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.8",
    "vite": "^6.2.2",
    "xlsx": "0.18.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-port-killer": "^1.2.1",
    "eslint-config-react-app": "^7.0.1"
  },
  "type": "module",
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.36.0"
  }
}
